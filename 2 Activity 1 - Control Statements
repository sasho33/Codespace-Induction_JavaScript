var a = prompt('Enter first number');
var b = prompt('Enter second number');
var c = prompt('Enter third number');

if ((a == b) == c) {
  console.log('All three numbers are equal');
} else if (a != b && b != c) {
  console.log('All three numbers are non equal');
} else {
  console.log('Neither all are equal or different');
}
